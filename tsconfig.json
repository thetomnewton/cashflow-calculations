{
    "compilerOptions": {
        "allowJs": false,
        "composite": true,
        "declaration": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "noImplicitOverride": true,
        "strict": true,
        "isolatedModules": true,
        "noEmit": true,
        "noImplicitAny": true,
        "target": "ES2020"
    },
    "include": ["src"]
}
